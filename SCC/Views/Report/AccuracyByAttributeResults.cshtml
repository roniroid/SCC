@{
    bool isControllable =
        Model.RequestObject.AttributeControllable != null
            ? Model.RequestObject.AttributeControllable.Value
            : false;
}

<div class="tableContainer mt-4">
    <table class="display cell-border text-center" id="tbInfo">
        <thead>
            <tr>
                <th class="text-center">
                    Atributo
                </th>
                <th class="text-center">
                    Cantidad
                </th>
                <th class="text-center">
                    Total
                </th>
                <th class="text-center">
                    Porcentaje
                </th>
            </tr>
        </thead>
        <tbody>
            @{
                List<int> processedID = new List<int>();
            }

            @foreach (SCC_BL.Reports.Results.AccuracyByAttribute accuracyByAttributeResult in Model.AccuracyByAttributeResultList.OrderBy(e => e.AttributeID))
            {
                if (processedID.Where(e => e == accuracyByAttributeResult.AttributeID).Count() <= 0)
                {
                    int successfulResultCount = 
                        Model.AccuracyByAttributeResultList
                            .Where(e => 
                                e.AttributeID == accuracyByAttributeResult.AttributeID && 
                                e.SuccessFulResult &&
                                (isControllable 
                                    ? !e.IsControllable
                                    : true))
                            .Count();

                    <tr>
                        <td class="text-start">
                            @accuracyByAttributeResult.AttributeName
                        </td>
                        <td>
                            @successfulResultCount
                        </td>
                        <td>
                            @Model.TotalTransactions
                        </td>
                        <td>
                            @String.Format(CultureInfo.InvariantCulture, "{0:0.##}", ((Convert.ToDecimal(successfulResultCount) / Model.TotalTransactions) * 100)).Replace('.', ',')%
                        </td>
                    </tr>
                }

                processedID.Add(accuracyByAttributeResult.AttributeID);
            }
        </tbody>
    </table>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.18.5/xlsx.full.min.js"></script>

